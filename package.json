{
  "name": "WhatBeer",
  "version": "1.0.0",
  "description": "A Vue, Nuxt, and Vuetify app utilizing the RateBeer GraphQL API.",
  "author": "Clark Mitchell",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
