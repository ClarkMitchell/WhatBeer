    "apollo-link": "^1.2.1",
    "apollo-link-http": "^1.5.3",
    "es6-promise": "^4.2.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "nuxt": "^1.0.0",
    "vue-apollo": "^3.0.0-beta.5",
    "vuetify": "^0.17.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "cross-env": "^5.0.1",
    "eslint": "^4.15.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "postcss-loader": "2.1.1",
